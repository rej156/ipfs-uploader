bs_package_flags = -bs-package-map my-app
src_root_dir = /Users/rej156/Documents/my-app
bsc = /Users/rej156/Documents/my-app/node_modules/bs-platform/lib/bsc.exe
bsdep = /Users/rej156/Documents/my-app/node_modules/bs-platform/lib/bsb_helper.exe
warnings = -w -30-40+6+7+27+32..39+44+45+101
bsc_flags = -bs-suffix -nostdlib -I '/Users/rej156/Documents/my-app/node_modules/bs-platform/lib/ocaml' -color always
ppx_flags = 
bs_package_includes = -I /Users/rej156/Documents/my-app/node_modules/reason-react/lib/ocaml -I /Users/rej156/Documents/my-app/node_modules/@aantron/repromise/lib/ocaml
bs_package_dev_includes = 
namespace = -ns MyApp
bsb_dir_group = 0
refmt = /Users/rej156/Documents/my-app/node_modules/bs-platform/lib/refmt.exe
reason_react_jsx = -ppx '/Users/rej156/Documents/my-app/node_modules/bs-platform/lib/reactjs_jsx_ppx_2.exe'
refmt_flags = --print binary
bsc_lib_includes = -I . -I src/lib -I src
rule build_ast_and_module_sets_from_re
  command = ${bsc} -pp "${refmt} ${refmt_flags}" ${reason_react_jsx}  ${ppx_flags} ${warnings} ${bsc_flags} -c -o ${out} -bs-syntax-only -bs-binary-ast -impl ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/App.mlast : build_ast_and_module_sets_from_re $src_root_dir/src/App.re
rule build_deps
  command = ${bsdep} ${namespace} -g ${bsb_dir_group} -MD ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/App.mlast.d : build_deps src/App.mlast
rule build_cmj_cmi
  command = ${bsc} ${bs_package_flags} -bs-assume-no-mli -bs-no-builtin-ppx-ml -bs-no-implicit-include ${bs_package_includes} ${bsc_lib_includes} ${bsc_extra_includes} ${warnings} ${bsc_flags} -o ${out} -c  ${in} $postbuild
  depfile = ${in}.d
  description = [34mBuilding[39m [2m${out}[22m
build src/App-MyApp.cmj |  $src_root_dir/src/App.bs.js src/App-MyApp.cmi : build_cmj_cmi src/App.mlast
  bsc_flags = $bsc_flags -bs-re-out -bs-super-errors
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:src
build src/BrowserWeb3Capabilities.mlast : build_ast_and_module_sets_from_re $src_root_dir/src/BrowserWeb3Capabilities.re
build src/BrowserWeb3Capabilities.mlast.d : build_deps src/BrowserWeb3Capabilities.mlast
build src/BrowserWeb3Capabilities-MyApp.cmj |  $src_root_dir/src/BrowserWeb3Capabilities.bs.js src/BrowserWeb3Capabilities-MyApp.cmi : build_cmj_cmi src/BrowserWeb3Capabilities.mlast
  bsc_flags = $bsc_flags -bs-re-out -bs-super-errors
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:src
build src/lib/ThreeBox.mlast : build_ast_and_module_sets_from_re $src_root_dir/src/lib/ThreeBox.re
build src/lib/ThreeBox.mlast.d : build_deps src/lib/ThreeBox.mlast
build src/lib/ThreeBox-MyApp.cmj |  $src_root_dir/src/lib/ThreeBox.bs.js src/lib/ThreeBox-MyApp.cmi : build_cmj_cmi src/lib/ThreeBox.mlast
  bsc_flags = $bsc_flags -bs-re-out -bs-super-errors
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:src/lib
rule build_package
  command = ${bsc} -w -49 -no-alias-deps -c ${in}
  description = [34mBuilding[39m [2m${out}[22m
build MyApp.cmi : build_package MyApp.mlmap
build build.ninja : phony  ||  MyApp.cmi src/lib/ThreeBox.mlast.d src/BrowserWeb3Capabilities.mlast.d src/App.mlast.d
